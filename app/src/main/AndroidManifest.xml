<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dell.coffeestore">

    <application
        android:allowBackup="true"
        android:icon="@drawable/coffeetubiao"
        android:label="@string/app_name"
        android:roundIcon="@drawable/coffeetubiao"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.MainActivity" />
        <activity android:name=".Fragment.HomeFragment" />
        <activity android:name=".Fragment.MeFragment" />
        <activity android:name=".Fragment.SearchFragment" />
        <activity android:name=".Fragment.ShopCarFragment" />
        <activity android:name=".Fragment.SortFagment" />
        <activity android:name=".Activity.CoffeeActivity">
            <intent-filter>
                <action android:name="com.great.activity_intent.coffeeActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.LoginActivity">

        </activity>
        <activity android:name=".Activity.Coffee_userActivity">
            <intent-filter>
                <action android:name="com.great.activity_intent.coffee_userActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.Coffee_informationActivity">
            <intent-filter>
                <action android:name="com.great.activity_intent.coffee_informationActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.RegisterActivity" />
        <activity android:name=".Activity.FirshCoffee">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.Coffee_Idea">

        </activity>
        <activity android:name=".Activity.Style">

        </activity>
    </application>

</manifest>